										<button id="editBtn" type="button" style="border: none; background-color:transparent;"onclick="javascript:location.href='/editAct/<%= results.cact[i].sactno %>'">
											<i class="fa fa-pencil-alt" style="font-size: 16px; color: #404040;"></i>
										</button>
										<button id="deleteBtn" style="border: none; background-color:transparent;"onclick="location.href='/deleteAct/<%= results.cact[i].sactno %>'">
